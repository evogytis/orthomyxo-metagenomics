	journal = {Viruses},
	keywords = {\textit{Orthomyxoviridae}, amphibians, evolution, fish, influenza, metatranscriptomics, phylogeny},
	language = {en},
	month = sep,
	note = {Number: 9 Publisher: Multidisciplinary Digital Publishing Institute},
	number = {9},
