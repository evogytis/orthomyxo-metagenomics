	issn = {0016-6731, 1943-2631},
	journal = {Genetics},
	language = {en},
	month = may,
	number = {1},
	pages = {141--150},
	pmid = {21339477},
	shorttitle = {Rhabdoviruses in {Two} {Species} of {Drosophila}},
	title = {Rhabdoviruses in {Two} {Species} of {Drosophila}: {Vertical} {Transmission} and a {Recent} {Sweep}},
