	issn = {1935-2735},
	journal = {PLOS Neglected Tropical Diseases},
	keywords = {Arboviruses, Metagenomics, Viral pathogens, Mosquitoes, West Nile virus, Genomics, Japanese encephalitis virus, Togaviruses},
