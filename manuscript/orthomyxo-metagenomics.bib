	journal = {Bioinformatics},
	month = jul,
	number = {14},
	pages = {1754--1760},
	title = {Fast and accurate short read alignment with {Burrows}--{Wheeler} transform},
	url = {https://doi.org/10.1093/bioinformatics/btp324},
	urldate = {2021-01-08},
	volume = {25},
	year = {2009},
