	journal = {Virologica Sinica},
	language = {eng},
	month = feb,
	number = {1},
	pages = {59--66},
	pmcid = {PMC6178073},
	pmid = {29500689},
	title = {Comparative {Metagenomic} {Profiling} of {Viromes} {Associated} with {Four} {Common} {Mosquito} {Species} in {China}},
	volume = {33},
