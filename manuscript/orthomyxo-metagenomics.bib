	issn = {1471-0064},
	journal = {Nature Reviews Genetics},
	keywords = {Agriculture, Animal Genetics and Genomics, Biomedicine, Cancer Research, Gene Function, general, Human Genetics},
	language = {en},
	month = mar,
	note = {Number: 3 Publisher: Nature Publishing Group},
	number = {3},
