	journal = {Journal of General Virology},
	number = {3},
	pages = {714--725},
	title = {Diagnosis and discovery of fungal viruses using deep sequencing of small {RNAs}},
	url = {https://jgv.microbiologyresearch.org/content/journal/jgv/10.1099/jgv.0.000003},
