	doi = {10.1038/s41586-019-0955-3},
	file = {Full Text PDF:/Users/evogytis/Zotero/storage/358S7DG8/Karakus et al. - 2019 - MHC class II proteins mediate cross-species entry .pdf:application/pdf;Snapshot:/Users/evogytis/Zotero/storage/KIAGKFU8/s41586-019-0955-3.html:text/html},
	issn = {1476-4687},
