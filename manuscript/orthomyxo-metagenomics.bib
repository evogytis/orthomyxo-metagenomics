	journal = {BMC Evolutionary Biology},
	month = oct,
	pages = {221},
	pmcid = {PMC3853548},
	pmid = {24093883},
	shorttitle = {Looking for trees in the forest},
	title = {Looking for trees in the forest: summary tree from posterior samples},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3853548/},
	urldate = {2019-05-31},
	volume = {13},
	year = {2013},
	Bdsk-Url-1 = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3853548/},
