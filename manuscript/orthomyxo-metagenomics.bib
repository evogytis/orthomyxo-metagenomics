	pmid = {29340209},
	shorttitle = {Discovery of {Culex} pipiens associated tunisia virus},
	title = {Discovery of {Culex} pipiens associated tunisia virus: a new {ssRNA}(+) virus representing a new insect associated virus family},
	volume = {4},
