	issn = {2041-1723},
	journal = {Nature Communications},
	keywords = {Viral evolution, Cryoelectron microscopy, Virus structures},
	language = {en},
	month = sep,
	note = {Number: 1 Publisher: Nature Publishing Group},
	number = {1},
	pages = {4646},
