	file = {Submitted Version:/Users/evogytis/Zotero/storage/6JRTFN5E/Konstantinidis et al. - 2021 - Defining Virus-Carrier Networks That Shape the Com.pdf:application/pdf},
	institution = {In Review},
	month = feb,
	title = {Defining {Virus}-{Carrier} {Networks} {That} {Shape} the {Composition} of the {Mosquito} {Core} {Virome} of an {Ecosystem}},
	type = {preprint},
	url = {https://www.researchsquare.com/article/rs-229254/v1},
	urldate = {2021-05-19},
