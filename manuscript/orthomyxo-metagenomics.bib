	issn = {0027-8424, 1091-6490},
	journal = {Proceedings of the National Academy of Sciences},
	keywords = {arboviruses, massively parallel sequencing, piRNAs, siRNAs, viral immunity},
	language = {en},
	month = jan,
	number = {4},
	pages = {1606--1611},
